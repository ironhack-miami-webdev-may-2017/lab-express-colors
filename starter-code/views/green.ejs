<h2> <%= myTitle %> </h2>

    <ul>
    <%  colorsList.forEach((oneColor) =>{ %>
    <%  if ( oneColor.category === 'green'){ %>


        <li>
          <div class ="color-box" style= "background-color:<%= oneColor.hex %>;"> </div>
          <p class = "text">
            <%= oneColor.keyword %>
            <br>
            <%= oneColor.hex %>
            <br>
            <%= oneColor.category %>
           </p>

        </li>
    <%  } %>

    <%  }) %>

    </ul>
